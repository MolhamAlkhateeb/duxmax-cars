{
  "name": "DuxMax Dev Container",
  "dockerComposeFile": "../docker-compose.dev.yml",
  "service": "postgres",  // Your main service (can be any service)
  "workspaceFolder": "/workspace",  // ðŸ‘ˆ Default path in Codespaces
  "forwardPorts": [5432, 6379, 5050],
  "remoteEnv": {
    "DATABASE_URL": "postgresql://postgres:postgres@postgres:5432/duxmax"
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-azuretools.vscode-docker"]
    }
  }
}